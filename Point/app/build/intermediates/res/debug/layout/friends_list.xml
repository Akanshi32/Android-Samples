<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:weightSum="1">

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:background="#088A08">

        <GridLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content">

            <ImageView
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:id="@+id/settings_img_btn"
                android:layout_row="0"
                android:layout_column="0"
                android:focusableInTouchMode="false"
                android:src="@drawable/configuration17_w"
                android:nestedScrollingEnabled="true"
                android:paddingLeft="13dp"
                android:paddingTop="0dp"
                android:paddingRight="7dp"
                android:paddingBottom="0dp" />

            <ImageView
                android:layout_width="45dp"
                android:layout_height="50dp"
                android:id="@+id/imageView2"
                android:layout_row="0"
                android:layout_column="3"
                android:src="@drawable/alarm45_w"
                android:paddingLeft="13dp"
                android:paddingRight="7dp"
                android:paddingTop="0dp"
                android:paddingBottom="0dp" />

            <ImageView
                android:layout_width="45dp"
                android:layout_height="50dp"
                android:id="@+id/add_friend_btn"
                android:layout_row="0"
                android:layout_column="4"
                android:src="@drawable/add174_w"
                android:paddingRight="13dp"
                android:paddingLeft="5dp" />
        </GridLayout>
    </TableRow>

    <TableRow
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="#088A08">

        <GridLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_marginBottom="1dp"
            android:background="#ffffffff">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="RECENT"
                android:id="@+id/textView"
                android:paddingLeft="20dp"
                android:paddingRight="20dp"
                android:paddingTop="7dp"
                android:paddingBottom="7dp"
                android:textSize="14dp" />
        </GridLayout>
    </TableRow>

    <TableRow
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="#f1f1f1">

        <GridLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:background="#ffffffff"
            android:layout_marginBottom="1dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="Keith Weaver"
                android:id="@+id/textView2"
                android:layout_row="0"
                android:layout_column="0"
                android:paddingLeft="20dp"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:paddingRight="20dp" />

            <ImageView
                android:layout_width="50dp"
                android:layout_height="40dp"
                android:id="@+id/imageView"
                android:layout_row="0"
                android:layout_column="2"
                android:src="@drawable/arrow613_g"
                android:paddingTop="8dp"
                android:paddingBottom="8dp" />
        </GridLayout>

    </TableRow>

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#088A08"
        android:layout_gravity="center_horizontal">

        <GridLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_marginBottom="1dp"
            android:background="#ffffffff"
            android:layout_marginTop="1dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="A"
                android:id="@+id/textView3"
                android:paddingLeft="20dp"
                android:paddingRight="20dp"
                android:paddingTop="7dp"
                android:paddingBottom="7dp"
                android:textSize="14dp" />
        </GridLayout>
    </TableRow>

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#f1f1f1"
        android:layout_gravity="center_horizontal">

        <GridLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:background="#ffffffff"
            android:layout_marginBottom="1dp" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="Keith Weaver"
                android:id="@+id/textView4"
                android:layout_row="0"
                android:layout_column="0"
                android:paddingLeft="20dp"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:paddingRight="20dp" />

            <ImageView
                android:layout_width="50dp"
                android:layout_height="40dp"
                android:id="@+id/imageView4"
                android:layout_row="0"
                android:layout_column="2"
                android:src="@drawable/arrow613_g"
                android:paddingTop="8dp"
                android:paddingBottom="8dp" />
        </GridLayout>
    </TableRow>

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="right"
        android:background="#ffffffff"></TableRow>
</LinearLayout><!-- From: file:/Users/Weaver/Desktop/my.android/github/android-learning/Point/app/src/main/res/layout/friends_list.xml -->