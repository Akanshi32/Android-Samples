<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".MainActivity">
    <TextView android:text="@string/hello_world" android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView" />
    <TextView android:text="@string/keiths_name" android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Keith"
        android:layout_above="@+id/Hannah"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
    <TextView android:text="@string/hannahs_name" android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Hannah"
        android:layout_above="@+id/Travis"
        android:layout_alignLeft="@+id/Chris"
        android:layout_alignStart="@+id/Chris"
        android:layout_marginBottom="18dp" />
    <TextView android:text="@string/chriss_name" android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Chris"
        android:layout_below="@+id/textView2"
        android:layout_alignLeft="@+id/textView2"
        android:layout_alignStart="@+id/textView2"
        android:layout_marginTop="43dp" />
    <TextView android:text="@string/traviss_name" android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Travis"
        android:layout_below="@+id/Chris"
        android:layout_alignRight="@+id/textView2"
        android:layout_alignEnd="@+id/textView2" />

    <TextView
        android:text="@string/hello_world"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView2"
        android:layout_below="@+id/textView"
        android:layout_alignRight="@+id/Keith"
        android:layout_alignEnd="@+id/Keith"
        android:layout_marginTop="26dp" />

</RelativeLayout>
